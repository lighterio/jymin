{
  "name": "jymin",
  "version": "0.4.0",
  "description": "Minifier-friendly JavaScript built for speed and usefulness",
  "dependencies": {},
  "devDependencies": {
    "exam": "^0.2.1",
    "coveralls": "2.10.0",
    "chug": "^0.3.0",
    "figlet": "1.0.9"
  },
  "keywords": [
    "jymin",
    "javascript",
    "you",
    "minify",
    "fast",
    "minifiable",
    "jquery",
    "zepto",
    "alternative"
  ],
  "main": "jymin.node.js",
  "engines": {
    "node": ">=0.2.6"
  },
  "scripts": {
    "build": "node .chug.js",
    "test": "./node_modules/exam/exam.js",
    "cover": "istanbul cover ./node_modules/exam/exam.js",
